{
  "name": "@zahnom/vsc-file-tags-core",
  "version": "1.0.0",
  "main": "bundle.js",
  "files": [
    "public/**/*"
  ],
  "publishConfig": {
  	"registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "postinstall": "dotnet tool restore",
    "start": "dotnet fable watch src --run webpack-dev-server",
    "build": "dotnet fable src && webpack",
    "pretest": "fable-splitter test -o dist/tests --commonjs",
    "test": "mocha dist/tests"
  },
  "devDependencies": {
    "fable-splitter": "^2.2.1",
    "mocha": "^10.1.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.11.2"
  },
  "version": "1.0.0"
}